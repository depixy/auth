{
  "name": "@depixy/auth",
  "version": "2.3.0",
  "description": "",
  "main": "./lib/cjs/index.js",
  "typings": "./lib/cjs/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module node16 --outDir lib/esm",
    "build:cjs": "tsc --module CommonJS --outDir lib/cjs",
    "lint": "eslint --ext .ts src",
    "test": "npm run test:esm && npm run test:cjs",
    "test:esm": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.esm.config.js",
    "test:cjs": "jest --config jest.cjs.config.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "keywords": [
    "depixy",
    "depixy-plugin"
  ],
  "homepage": "https://github.com/depixy/auth#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/depixy/auth.git"
  },
  "bugs": {
    "url": "https://github.com/depixy/auth/issues"
  },
  "author": "Joshua Avalon",
  "license": "Apache-2.0",
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {
    "argon2": "^0.28.5",
    "fastify-plugin": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.4.0"
  },
  "devDependencies": {
    "@depixy/database": "^2.3.0",
    "@fastify/cookie": "^7.0.0",
    "@joshuaavalon/eslint-config-typescript": "^6.1.4",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/luxon": "^2.3.2",
    "@types/node": "^16.11.39",
    "eslint": "^8.17.0",
    "fastify": "^4.0.1",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3"
  }
}
